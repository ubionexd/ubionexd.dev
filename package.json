{
  "name": "simple-static-nuxtjs-blog",
  "version": "1.0.0",
  "description": "A template to quickstart your next blog",
  "author": "Milchreis",
  "private": true,
  "scripts": {
    "dev": "nuxt --openssl-legacy-provider",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "generate:gh-pages": "cross-env DEPLOY_ENV='GH_PAGES' nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@fortawesome/vue-fontawesome": "^2.0.8",
    "@nuxtjs/sitemap": "^2.4.0",
    "nuxt": "^2.14.7",
    "vue-material-design-icons": "^5.1.1"
  },
  "devDependencies": {
    "@nuxtjs/google-analytics": "^2.4.0",
    "cross-env": "^7.0.2",
    "frontmatter-markdown-loader": "^3.6.1",
    "push-dir": "^0.4.1"
  }
}
